{
  "name": "equipamentos-sirgs-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run docker:up && node src/scripts/wait-for-rabbitmq.js && npm run start:all",
    "start:pm2": "npm run docker:up && node src/scripts/wait-for-rabbitmq.js && pm2 start ecosystem.config.js",
    "start:all": "concurrently \"npm run start:api\" \"npm run start:worker\"",
    "start:api": "node dist/index.js",
    "start:worker": "node dist/wokers/logsWorker.js",
    "dev": "npm run docker:up && node src/scripts/wait-for-rabbitmq.js && npm run dev:all",
    "dev:all": "concurrently --names \"API,WORKER\" --prefix-colors \"blue,green\" \"npm run dev:api\" \"npm run dev:worker\"",
    "dev:api": "tsx watch src/index.ts",
    "dev:worker": "tsx watch src/wokers/logsWorker.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "build": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "amqplib": "^0.10.9",
    "amqplib-as-promised": "^4.1.0",
    "bcrypt": "^6.0.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "prisma": "^5.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.4",
    "typescript": "^5.0.0"
  }
}
